    "typescript",
    "postcss",
    "tailwind"
  ],
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@types/ejs": "3.1.0",
    "@types/hash-sum": "1.0.0",
    "@types/ws": "8.2.2",
    "@vue/compiler-sfc": "3.2.26",
    "autoprefixer": "10.4.1",
    "bundle-require": "2.2.0",
    "cac": "6.7.12",
    "dotenv": "11.0.0",
    "dotenv-expand": "5.1.0",
    "h3": "0.3.8",
    "hash-sum": "2.0.0",
    "lodash": "4.17.21",
