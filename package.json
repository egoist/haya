    "time-span": "5.0.0",
    "ts-essentials": "9.1.2",
    "tsup": "5.11.10",
    "typescript": "4.5.4",
    "upath": "2.0.1",
    "vue": "3.2.26",
    "vue-router": "4.0.12"
