{
  "name": "haya",
  "version": "0.0.10",
  "description": "esbuild based dev server and production bundler",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
